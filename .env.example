# Telegram Bot Configuration
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Bot username for captions (default: @white_cat_downloader_bot)
BOT_USERNAME=@your_bot_username

# Optional: Logging level (default: WARNING)
# Valid values: DEBUG, INFO, WARNING, ERROR, CRITICAL
# LOG_LEVEL=INFO

# Optional: Enable health check server (default: true)
ENABLE_HEALTH_CHECK=true

# Optional: HTTP server port for health checks (default: 8080, auto-set by Render)
# PORT=8080

# Service Priorities (optional, 0-100, higher = tried first)
INSTAGRAM_PRIORITY=80
TIKTOK_PRIORITY=70

# Instagram Provider API Keys & Priorities
INSTAGRAM120_API_KEY=your_rapidapi_key_here
INSTAGRAM120_PRIORITY=80

INSTAGRAM_DOWNLOADER_API_KEY=your_rapidapi_key_here
INSTAGRAM_DOWNLOADER_PRIORITY=50

INSTAGRAM_LOOTER2_API_KEY=your_rapidapi_key_here
INSTAGRAM_LOOTER2_PRIORITY=35

# TikTok Provider API Keys & Priorities
TIKTOK_API1_API_KEY=your_rapidapi_key_here
TIKTOK_API1_PRIORITY=90

TIKTOK_NOWATERMARK2_API_KEY=your_rapidapi_key_here
TIKTOK_NOWATERMARK2_PRIORITY=85
